     ]
    }
   ],
   "source": [
    "\n",
    "n = int(input())\n",
    "\n",
    "\n",
    "\n",
    "#initialize the sum\n",
    "\n",
    "s = 0\n",
    "\n",
    "\n",
    "\n",
    "t = n\n",
    "\n",
    "while t > 0:\n",
    "\n",
    "   digit = t % 10\n",
    "\n",
    "   s += digit ** 3\n",
    "\n",
    "   t //= 10\n",
    "\n",
    "\n",
    "\n",
    "# display the result\n",
    "\n",
    "if n == s:\n",
    "\n",
    "   print(n,\"is an Armstrong number\")\n",
    "\n",
    "else:\n",
    "\n",
    "   print(n,\"is not an Armstrong number\")\n",
    "\n",
    "\n",
    "\n"
   ]
